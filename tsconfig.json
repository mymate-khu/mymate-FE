{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "target": "ESNext",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "types": [
      "react",
      "react-native",
      "react-dom",
      "expo"
    ]
  },
  "extends": "expo/tsconfig.base"
}
